pipeline {
    agent any
    stages {
        stage('Coding Stage') {
            steps {
                echo 'This is the Coding Stage'
            }
        }
        stage('Build') {
            steps {
                echo 'This is the Build Stage'
            }
        }
        stage('Testing') {
            steps {
                echo 'This is the Test Confirmation Stage'
            }
        }
        stage('Release') {
            steps {
                echo 'This is the release Confirmation Stage'
            }
        }
    
        stage('Production') {
            steps {
                echo 'This is the Production release Confirmation Stage'
            }
        }
    }
}
